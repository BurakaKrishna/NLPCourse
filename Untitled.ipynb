    "import re\n",
    "dateReg=re.compile(r'(\\d{2}[-\\\\]){2}\\d{4}')\n",
    "def tokenize(sentence):\n",
    "    sentence = dateReg.sub(\"_DATE_\",sentence)\n",
    "    tokens = sentence.split()\n",
    "    return tokens\n",
